
set out 'bra-fin.png'
set term png size 1024,768
# set term png size 640, 480

set title "Height injection"
set logscale x
set logscale y
unset xtics
set xrange [256:512]
plot "bra-fin.dat" using ($1*4):4 with lines linewidth 2 title "64-128", \
     "bra-fin.dat" using 1:4 with lines linewidth 2 title "256-512", \
     "bra-fin.dat" using ($1/4):4 with lines linewidth 2 title "2048-4096"

